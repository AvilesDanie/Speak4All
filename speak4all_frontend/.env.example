# Speak4All frontend environment example
# Copy this file to .env and fill real values before running

# Google OAuth credentials (create in Google Cloud Console)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# NextAuth base URL (frontend public url)
NEXTAUTH_URL=http://localhost:3000

# Internal URL used by NextAuth when running in Docker (optional)
NEXTAUTH_URL_INTERNAL=http://frontend:3000
